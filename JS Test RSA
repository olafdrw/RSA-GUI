// Test

var modInverse = function(a, b) {
    a %= b;
    for (var x = 1; x < b; x++) {
        if ((a*x)%b == 1) {
            return x;
        }
    }
}

function generate_keyPair() {
  var p = 59
  var q = 53
  var n = p*q;
  const e = Math.pow(2,16) + 1;
  var phi = (p-1)*(q-1);
  var d = modInverse(e, phi);
  console.log(phi);
  console.log(d);
}



console.log(generate_keyPair())
